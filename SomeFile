var user = System.Input();
System.InitUser(user);

public void DestroyMatrix()
{
  this.System.Destroy();
  if(System.End)
  {
    System.Notify();
  }
}
if(System.State.DestroyStart)
{
  System.Crash.Cancel();
}

public bool FindTarget(Object directory)
{
    System.AddDestroyFlag(directory, true);
}
